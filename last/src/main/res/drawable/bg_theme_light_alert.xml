<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <stroke
                android:color="#ADA2A2"
                android:width="6dp" />
        </shape>
    </item>
    <item
        android:left="3dp"
        android:right="3dp"
        android:top="3dp"
        android:bottom="3dp">
        <shape android:shape="rectangle">
            <corners android:radius="8dp" />
            <stroke
                android:color="#504A4A"
                android:width="2dp" />
        </shape>
    </item>
    <item
        android:left="4dp"
        android:right="4dp"
        android:top="4dp"
        android:bottom="4dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white_teeth" />
            <corners android:radius="8dp" />
        </shape>
    </item>

</layer-list>